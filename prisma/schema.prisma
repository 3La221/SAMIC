generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Objectif {
  id Int @id @default(autoincrement())
  label String 
}

model Mission {
  id Int @id @default(autoincrement())
  label String 
}


model Organisation {
  id Int @id @default(autoincrement())
  title String
  desc String
  img String
}


model Actualite {
  id Int @id @default(autoincrement())
  title String 
  date String
  img String
}

model Event {
  id Int @id @default(autoincrement())
  title String 
  date String
  desc String
}

model Formation {
  id Int @id @default(autoincrement())
  title String 
  date String
  desc String
}

