generator client {
  provider = "prisma-client-js"

}

generator jsonSchema {
  provider = "prisma-json-schema-generator"
  includeRequiredFields = "true"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Objectif {
  id Int @id @default(autoincrement())
  label String 
}

model Mission {
  id Int @id @default(autoincrement())
  label String 
}


